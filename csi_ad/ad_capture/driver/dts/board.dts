/dts-v1/;

#include <dt-bindings/gpio/sun4i-gpio.h>

#include "sun8iw11p1.dtsi"

/{
	model = "TLT3/A40iF-EVM HDMI";

	aliases {
		pmu0 = &pmu0;
		standby_param = &standby_param;
		ethernet0 = &gmac0;
		ethernet1 = &emac0;
	};

	clk_sit2515: sit2515 {
		#clock-cells = <0>;
		compatible = "fixed-clock";
		clock-frequency = <16000000>;
		clock-output-names = "sit2515";
	};

	soc@1c00000 {
		card0_boot_para@2 {
		/* Avoid dtc compiling warnings. */
		reg = <0x0 0x2 0x0 0x0>;
		device_type = "card0_boot_para";
		card_ctrl = <0x0>;
		card_high_speed = <0x1>;
		card_line = <0x4>;
		pinctrl-0 = <&sdc0_pins_uboot>;
		};

		card2_boot_para@3 {
			/* Avoid dtc compiling warnings. */
			reg = <0x0 0x3 0x0 0x0>;
			device_type = "card2_boot_para";
			card_ctrl = <0x2>;
			card_high_speed = <0x1>;
			card_line = <0x8>;
			pinctrl-0 = <&sdc2_pins_uboot>;
			sdc_ex_dly_used = <0x2>;
			sdc_io_1v8 = <0x1>;
			sdc_type = "tm4";
		};

		platform@1 {
			eraseflag   = <1>;
			next_work   = <3>;
			debug_mode  = <1>;
		};
	};

	leds {
		compatible = "gpio-leds";
		status = "okay";

		led@0 {
			label = "heartbeat";
			gpios = <&pio PC 17 0>;   /* PC17 */
			linux,default-trigger = "heartbeat";
		};

		led@1 {
			label = "disk";
			gpios = <&pio PC 18 0>;   /* PC18 */
			linux,default-trigger = "mmc0";
		};

		led@2 {
			label = "user-led0";
			gpios = <&pio PB 4 0>;   /* PB4 */
			default-state = "on";
		};

		led@3 {
			label = "user-led1";
			gpios = <&pio PB 10 0>;   /* PB10 */
			default-state = "on";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		user_key@0 {
			label = "user-key0";
			linux,code = <202>; /* KEY0-KEY_PROG3 */
			gpios = <&pio PH 13 1>;
			debounce-interval = <0>;
		};

		user_gpio@0 {
			label = "user-gpio0";
			linux,code = <194>; /* GPIO0-KEY_F24 */
			gpios = <&pio PH 2 1>;
			debounce-interval = <0>;
		};
	};

	standby_param: standby_param {
		vdd-cpu = <0x00000002>;
		vdd-sys = <0x00000004>;
		vcc-pll = <0x00000000>;
		osc24m-on = <0x1>;
	};

	reg_usb1_vbus: usb1-vbus {
		compatible = "regulator-fixed";
		gpio = <&pio PI 11 GPIO_ACTIVE_LOW>;
		regulator-name = "usb1-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-enable-ramp-delay = <1000>;
		enable-active-low;
	};

};
&pio {

	uart0_pins_a: uart0_pins@0 {
		pins = "PB22", "PB23";
		function = "uart0";
	};

	uart0_pins_b: uart0_pins@1 {
		pins = "PB22", "PB23";
		function = "gpio_in";
	};
	uart1_pins_a: uart1_pins@0 {
		pins = "PA10", "PA11", "PA12", "PA13",
					 "PA14", "PA15", "PA16", "PA17";
		function = "uart1";
	};

	uart1_pins_b: uart1_pins@1 {
		pins = "PA10", "PA11", "PA12", "PA13",
					 "PA14", "PA15", "PA16", "PA17";
		function = "gpio_in";
	};

	uart2_pins_a: uart2_pins@0 {
		pins = "PI18", "PI19";
		function = "uart2";
	};

	uart2_pins_b: uart2_pins@1 {
		pins = "PI18", "PI19";
		function = "gpio_in";
	};

	uart3_pins_a: uart3_pins@0 {
		pins = "PH0", "PH1";
		function = "uart3";
	};

	uart3_pins_b: uart3_pins@1 {
		pins = "PH0", "PH1";
		function = "gpio_in";
	};

	uart4_pins_a: uart4_pins@0 {
		pins = "PH4", "PH5";
		function = "uart4";
	};

	uart4_pins_b: uart4_pins@1 {
		pins = "PH4", "PH5";
		function = "gpio_in";
	};

	uart5_pins_a: uart5_pins@0 {
		pins = "PH6", "PH7";
		function = "uart5";
	};

	uart5_pins_b: uart5_pins@1 {
		pins = "PH6", "PH7";
		function = "gpio_in";
	};

	uart6_pins_a: uart6_pins@0 {
		pins = "PI12", "PI13";
		function = "uart6";
	};

	uart6_pins_b: uart6_pins@1 {
		pins = "PI12", "PI13";
		function = "gpio_in";
	};

	uart7_pins_a: uart7_pins@0 {
		pins ="PI20", "PI21" ;
		function = "uart7";
	};

	uart7_pins_b: uart7_pins@1 {
		pins ="PI20", "PI21" ;
		function = "gpio_in";
	};

	spi0_pins_a: spi0@0 {
		pins = "PC2", "PC0", "PC1";     /*CLK  MOSI  MISO*/
		function = "spi0";
		muxsel = <3>;
		drive-strength = <10>;
	};

	spi0_pins_cs0: spi0@1 {
		pins = "PC23";    /*CS0*/
		function = "spi0";
		muxsel = <3>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	spi0_pins_cs1: spi0@2 {
		pins = "PI14";    /*CS1*/
		function = "spi0";
		muxsel = <3>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	spi0_pins_c: spi0@3 {
		pins = "PC2", "PC0", "PC1", "PC15", "PC16", "PC23";
		function = "gpio_in";
		muxsel = <0>;
		drive-strength = <10>;
	};

	spi1_pins_a: spi1@0 {
		pins = "PA1", "PA2", "PA3";     /*CLK  MOSI  MISO*/
		function = "spi1";
		muxsel = <3>;
		drive-strength = <10>;
	};

	spi1_pins_cs: spi1@1 {
		pins = "PA0";    /*CS0*/
		function = "spi1";
		muxsel = <3>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	spi2_pins_a: spi2@0 {
		pins = "PC20", "PC21", "PC22";     /*CLK  MOSI  MISO*/
		function = "spi2";
		muxsel = <3>;
		drive-strength = <10>;
	};

	spi2_pins_cs: spi2@1 {
		pins = "PC19", "PB13";    /*CS0*/
		function = "spi2";
		muxsel = <3>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	sdc0_pins_uboot: sdc0_uboot@0 {
		pins = "PF0", "PF1", "PF2", "PF3","PF4","PF5";
		function = "sdc0";
		muxsel = <3>;
		bias-pull-up;
		drive-strength = <30>;
	};

	sdc2_pins_uboot: sdc2_uboot@0 {
		pins = "PC5", "PC6","PC7", "PC8", "PC9","PC10","PC11","PC12","PC13","PC14","PC15","PC24";
		function = "sdc2";
		muxsel = <3>;
		bias-pull-up;
		drive-strength = <30>;
	};

	sdc0_pins_a: sdc0@0 {
	                    pins = "PF0", "PF1", "PF2",
	                           "PF3", "PF4", "PF5";
	                    function = "sdc0";
	                    drive-strength = <30>;
	                    bias-pull-up;
	            };

	sdc0_pins_b: sdc0@1 {
	        pins = "PF0", "PF1", "PF2",
	               "PF3", "PF4", "PF5";
	        function = "sdc0";
	        drive-strength = <30>;
	        bias-pull-up;
	};

	sdc0_pins_c: sdc0@2 {
	        pins = "PF0", "PF1", "PF2",
	               "PF3", "PF4", "PF5";
	        function = "gpio_in";
	};

	/* TODO: add jtag pin */
	sdc0_pins_d: sdc0@3 {
	        pins = "PF2", "PF4";
	        function = "uart0";
	        drive-strength = <10>;
	        bias-pull-up;
	};

	sdc0_pins_e: sdc0@4 {
	        pins = "PF0", "PF1", "PF3",
	                "PF5";
	        function = "jtag";
	        drive-strength = <10>;
	        bias-pull-up;
	};

	sdc1_pins_a: sdc1@0 {
		pins = "PH22", "PH23", "PH24", "PH25", "PH26", "PH27";
		function = "sdc1";
		drive-strength = <30>;
		bias-pull-up;
	};

	sdc1_pins_b: sdc1@1 {
		pins = "PH22", "PH23", "PH24", "PH25", "PH26", "PH27";
		function = "gpio_in";
		bias-pull-up;
	};

	sdc2_pins_a: sdc2@0 {
		pins = "PC6","PC7", "PC8", "PC9","PC10","PC11","PC12","PC13","PC14","PC15","PC24";
		function = "sdc2";
		drive-strength = <30>;
		bias-pull-up;
	};

	sdc2_pins_b: sdc2@1 {
		pins = "PC6","PC7", "PC8", "PC9","PC10","PC11","PC12","PC13","PC14","PC15","PC24";
		function = "gpio_in";
		bias-pull-up;
	};

	sdc2_pins_c: sdc2@2 {
		allwinner,pins = "PC5";
		allwinner,function = "sdc2";
		drive-strength = <30>;
		bias-pull-down;
	};

	sdc3_pins_a: sdc3@0 {
		pins = "PI4", "PI5","PI6", "PI7", "PI8","PI9";
		function = "sdc3";
		allwinner,drive = <3>;
		drive-strength = <30>;
		bias-pull-up;
	};

	sdc3_pins_b: sdc3@1 {
		pins = "PI4", "PI5","PI6", "PI7", "PI8","PI9";
		function = "io_disabled";
		allwinner,drive = <3>;
		drive-strength = <30>;
		bias-pull-up;
	};

	gmac_pins_a: gmac@0 {
		allwinner,pins = "PA0", "PA1", "PA2", "PA3",
				"PA4", "PA5", "PA6", "PA7",
				"PA8", "PA9", "PA10", "PA11",
				"PA11", "PA12", "PA13", "PA15";
		allwinner,function = "gmac0";
		allwinner,muxsel = <5>;
		allwinner,drive = <3>;
		allwinner,pull = <0>;
	};

	emac_pins_a: emac@0 {
		allwinner,pins = "PH8", "PH9", "PH10", "PH11",
				"PH14", "PH15", "PH16", "PH17",
				"PH18", "PH19", "PH20", "PH21",
				"PH22", "PH23", "PH24", "PH25",
				"PH26";
		allwinner,function = "emac0";
		allwinner,muxsel = <3>;
		allwinner,drive = <3>;
		allwinner,pull = <0>;
	};

	pwm0_pin_a: pwm0@0 {
		pins = "PB2";
		function = "pwm0";
		drive-strength = <10>;
		bias-pull-up;
	};

	pwm0_pin_b: pwm0@1 {
		pins = "PB2";
		function = "gpio_in";
		bias-disable;
	};

	pwm1_pin_a: pwm1@0 {
		pins = "PB3";
		function = "pwm1";
		drive-strength = <10>;
		bias-pull-up;
	};

	pwm1_pin_b: pwm1@1 {
		pins = "PB3";
		function = "gpio_in";
		bias-disable;
	};

	pwm4_pin_a: pwm4@0 {
		pins = "PB20";
		function = "pwm4";
		drive-strength = <10>;
		bias-pull-up;
	};

	pwm4_pin_b: pwm4@1 {
		pins = "PB20";
		function = "gpio_in";
		bias-disable;
	};

	twi0_pins_a: twi0@0 {
		pins = "PB0", "PB1";
		function = "twi0";
		drive-strength = <10>;
		bias-pull-up;
	};

	twi0_pins_b: twi0@1 {
		pins = "PB0", "PB1";
		function = "gpio_in";
	};

	twi1_pins_a: twi1@0 {
		pins = "PB18", "PB19";
		function = "twi1";
		drive-strength = <10>;
		bias-pull-up;
	};

	twi1_pins_b: twi1@1 {
		pins = "PB18", "PB19";
		function = "gpio_in";
	};

	twi2_pins_a: twi2@0 {
		pins = "PB20", "PB21";
		function = "twi2";
		drive-strength = <10>;
		bias-pull-up;
	};

	twi2_pins_b: twi2@1 {
		pins = "PB20", "PB21";
		function = "gpio_in";
	};

	twi3_pins_a: twi3@0 {
		pins = "PI0", "PI1";
		function = "twi3";
		drive-strength = <10>;
		bias-pull-up;
	};

	twi3_pins_b: twi3@1 {
		pins = "PI0", "PI1";
		function = "gpio_in";
	};

	twi4_pins_a: twi4@0 {
		pins = "PI2", "PI3";
		function = "twi4";
		drive-strength = <10>;
		bias-pull-up;
	};

	twi4_pins_b: twi4@1 {
		pins = "PI2", "PI3";
		function = "gpio_in";
	};

	spdif_pins_a: spdif@0 {
		pins = "PB3", "PB12", "PB13";
		function = "spdif0";
		drive-strength = <20>;
		bias-disable;
	};

	spdif_pins_b: spdif_sleep@0 {
		pins = "PB3", "PB12", "PB13";
		function = "io_disabled";
		drive-strength = <20>;
		bias-disable;
	};

	daudio0_pins_a: daudio0@0 {
		pins = "PB5", "PB6", "PB7", "PB8",
		       "PB9", "PB10", "PB11", "PB12";
		function = "i2s0";
		drive-strength = <20>;
		bias-disable;
	};

	daudio0_pins_b: daudio0_sleep@0 {
		pins = "PB5", "PB6", "PB7", "PB8",
		       "PB9", "PB10", "PB11", "PB12";
		function = "io_disabled";
		drive-strength = <20>;
		bias-disable;
	};

	daudio1_pins_a: daudio1@0 {
		pins = "PA9", "PA14", "PA15",
		       "PA16", "PA17";
		function = "i2s1";
		drive-strength = <20>;
		bias-disable;
	};

	daudio1_pins_b: daudio1_sleep@0 {
		pins = "PA9", "PA14", "PA15",
		       "PA16", "PA17";
		function = "io_disabled";
		drive-strength = <20>;
		bias-disable;
	};

	ir0_pins_a: ir0@0 {
		pins = "PB4";
		function = "ir0";
		drive-strength = <10>;
		bias-pull-up;
	};

	ir0_pins_b: ir0@1 {
		pins = "PB4";
		function = "gpio_in";
		bias-disable;
	};

	ir1_pins_a: ir1@0 {
		pins = "PB23";
		function = "ir1";
		drive-strength = <10>;
		bias-pull-up;
	};

	ir1_pins_b: ir1@1 {
		pins = "PB23";
		function = "gpio_in";
		bias-disable;
	};

	rgb18_pins_a: rgb18@0 {
		pins = "PD0", "PD1", "PD2", "PD3", "PD4", "PD5", \
			"PD6", "PD7", "PD8", "PD9", "PD10", "PD11", \
			"PD12", "PD13", "PD14", "PD15", "PD16", "PD17", \
			"PD18", "PD19", "PD20", "PD21", "PD22", "PD23", \
			"PD24", "PD25";
		function = "lcd0";
		drive-strength = <30>;
		bias-disable;
	};

	rgb18_pins_b: rgb18@1 {
		pins = "PD0", "PD1", "PD2", "PD3", "PD4", "PD5", \
			"PD6", "PD7", "PD8", "PD9", "PD10", "PD11", \
			"PD12", "PD13", "PD14", "PD15", "PD16", "PD17", \
			"PD18", "PD19", "PD20", "PD21", "PD22", "PD23", \
			"PD24", "PD25";
		function = "io_disabled";
		bias-disable;
	};

	vga_sync_a: vga_sync@0 {
		pins = "PD26", "PD27";
		function = "lcd0";
		drive-strength = <30>;
		bias-disable;
	};

	vga_sync_b: vga_sync@1 {
		pins = "PD26", "PD27";
		function = "io_disabled";
		drive-strength = <30>;
		bias-disable;
	};
};

&uart0 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart0_pins_a>;
	pinctrl-1 = <&uart0_pins_b>;
	status = "okay";
};

&uart1 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart1_pins_a>;
	pinctrl-1 = <&uart1_pins_b>;
	status = "disabled";
};

&uart2 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart2_pins_a>;
	pinctrl-1 = <&uart2_pins_b>;
	status = "okay";
};

&uart3 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart3_pins_a>;
	pinctrl-1 = <&uart3_pins_b>;
	status = "okay";
};

&uart4 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart4_pins_a>;
	pinctrl-1 = <&uart4_pins_b>;
	status = "okay";
};

&uart5 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart5_pins_a>;
	pinctrl-1 = <&uart5_pins_b>;
	/* rs485-enable = <0>;					*/
	/* rs485-mode = <0>;   /* defalut 0:rx 1:tx		*/
	/* rs485-mode-pin = <&pio PH 8 GPIO_ACTIVE_LOW>;	*/
	status = "okay";
};

&uart6 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart6_pins_a>;
	pinctrl-1 = <&uart6_pins_b>;
	status = "okay";
};

&uart7 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart7_pins_a>;
	pinctrl-1 = <&uart7_pins_b>;
	status = "okay";
};

&lradc{
	status = "okay";
	key_cnt = <5>;
	key0 = <190 148>;		/* KEY0-KEY_PROG1 */
	key1 = <390 114>;
	key2 = <600 139>;
	key3 = <800 28>;
	key4 = <980 102>;
	keyadc1_cnt = <5>;
	keyadc1_0 = <190 149>;		/* KEY0-KEY_PROG2 */
	keyadc1_1 = <390 203>;		/* KEY0-KEY_PROG4 */
	keyadc1_2 = <600 240>;		/* KEY0-KEY_UNKNOWN */
	keyadc1_3 = <800 240>;		/* KEY0-KEY_UNKNOWN */
	keyadc1_4 = <980 240>;		/* KEY0-KEY_UNKNOWN */
	wakeup-source;
};

&ir0 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&ir0_pins_a>;
	pinctrl-1 = <&ir0_pins_b>;
	status = "disabled";
};

&ir1 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&ir1_pins_a>;
	pinctrl-1 = <&ir1_pins_b>;
	status = "disabled";
};

&twi0 {
	clock-frequency = <400000>;
	pinctrl-0 = <&twi0_pins_a>;
	pinctrl-1 = <&twi0_pins_b>;
	pinctrl-names = "default", "sleep";
	no_suspend = <1>;
	status = "okay";
	pmu0: pmu@34 {
		compatible = "x-powers,axp221";
		reg = <0x34>;
		status = "okay";
		interrupts = <0 IRQ_TYPE_LEVEL_LOW>;
		interrupt-parent = <&nmi_intc>;
		x-powers,gpio-vbus-en;
		pmu_reset = <0>;
		pmu_irq_wakeup = <1>;
		pmu_hot_shutdown = <1>;

		wakeup-source;

		powerkey0: powerkey@0 {
			status = "okay";
			compatible = "x-powers,axp2101-pek";
			pmu_powkey_off_time = <6000>;
			pmu_powkey_off_func = <0>;
			pmu_powkey_off_en = <1>;
			pmu_powkey_long_time = <1500>;
			pmu_powkey_on_time = <1000>;
			wakeup_rising;
			wakeup_falling;
		};

		usb_power_supply: usb_power_supply {
			compatible = "x-powers,axp22x-usb-power-supply";
			status = "okay";

			pmu_usbpc_vol = <4400>;
			pmu_usbpc_cur = <500>;
			pmu_usbad_vol = <4000>;
			pmu_usbad_cur = <2500>;
			pmu_usb_typec_used = <1>;
			wakeup_usb_in;
			wakeup_usb_out;
		};

		ac_power_supply: ac-power-supply {
			compatible = "x-powers,axp22x-ac-power-supply";
			status = "okay";

			pmu_ac_vol = <4000>;
			pmu_ac_cur = <3000>;

			wakeup_ac_in;
			wakeup_ac_out;
		};

		bat_power_supply: bat_power_supply {
			compatible = "x-powers,axp22x-bat-power-supply";
			status = "okay";

			pmu_bat_unused = <1>;
			pmu_chg_ic_temp = <0>;
			pmu_battery_rdc= <100>;
			pmu_battery_cap = <4000>;
